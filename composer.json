{
  "name": "laravel-arcanist/inertia-response-renderer",
  "description": "Inertia response renderer for Arcanist",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Kai Sassnowski",
      "email": "me@kai-sassnowski.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "inertiajs/inertia-laravel": "^0.6.9",
    "laravel-arcanist/arcanist": "^0.7.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.30.2",
    "mockery/mockery": "^1.5.1",
    "orchestra/testbench": "^6.24.1 || ^7.22.1",
    "phpunit/phpunit": "^9.6.4",
    "roave/security-advisories": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Arcanist\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Arcanist\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Arcanist\\InertiaResponseRendererServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
